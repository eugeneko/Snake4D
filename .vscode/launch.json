{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(Windows) Snake4D Borderless",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/Debug/Source/Debug/Snake4D.exe",
            "args": ["-w", "--borderless", "--pp", "${workspaceFolder}/bin", "--pr", "CoreData;Data", "-m", "4"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build/Debug/Source/Debug",
            "visualizerFile": "${workspaceFolder}/3rdParty/rbfx/script/rbfx.natvis",
            "environment": [],
            "externalConsole": true
        },
        {
            "name": "(Windows) Snake4D Window",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/Debug/Source/Debug/Snake4D.exe",
            "args": ["-w", "-s", "--pp", "${workspaceFolder}/bin", "--pr", "CoreData;Data", "-m", "4"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build/Debug/Source/Debug",
            "visualizerFile": "${workspaceFolder}/3rdParty/rbfx/script/rbfx.natvis",
            "environment": [],
            "externalConsole": true
        }
    ]
}